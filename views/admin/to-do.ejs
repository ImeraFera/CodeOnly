<!DOCTYPE html>
<html lang="en">

    <head>
        <%- include('../admin-partials/header.ejs') %>
    </head>

    <body>
        <div class="container-xxl position-relative  d-flex p-0">

            <!-- Sidebar Start -->
            <%- include('../admin-partials/sidebar.ejs' , {user:data.user}) %>
            <!-- Sidebar End -->

            <!-- Content Start -->
            <div class="content bg-dark">
                <!-- Navbar Start -->
                <%- include('../admin-partials/navbar.ejs', {user:data.user}) %>
                <!-- Navbar End -->

                <!-- Widgets Start -->
                <div class="container-fluid pt-4 px-4">
                    <div class="row g-4">
                        <div class="col-md-12">
                            <div
                                class="bg-dark border border-2 border-primary rounded p-4">
                                <div
                                    class="d-flex align-items-center justify-content-between mb-4">
                                    <h6 class="mb-0 text-light">Add To Do </h6>
                                </div>
                                <div class="d-flex mb-2">
                                    <div class="input-group ">
                                        <input type="text"
                                            class="form-control bg-transparent text-light border border-2 border-primary"
                                            placeholder="Task">
                                        <button
                                            class="btn btn-outline-primary border border-2 border-primary   "
                                            type="button"
                                            id="button-addon2"><i
                                                class="fas fa-plus"></i></button>
                                    </div>

                                </div>

                                <div
                                    class="bg-dark text-white rounded h-100 p-4">
                                    <h6 class="mb-4 text-light">To Do List</h6>
                                    <table class="table table-borderless">
                                        <thead class="text-primary text-center">
                                            <tr>
                                                <th scope="col">#</th>
                                                <th scope="col">Task</th>
                                                <th scope="col">Action</th>
                                            </tr>
                                        </thead>
                                        <tbody class="text-white text-center">
                                            <tr>
                                                <th scope="row">1</th>
                                                <td>Example Task</td>
                                                <td>
                                                    <a
                                                        class="text-light "
                                                        href>
                                                        <i
                                                            class="fas fa-times"></i>
                                                    </a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <th scope="row">1</th>
                                                <td>Example Task</td>
                                                <td>
                                                    <a
                                                        class="text-light "
                                                        href>
                                                        <i
                                                            class="fas fa-times"></i>
                                                    </a>
                                                </td>
                                            </tr>
                                            <!-- Diğer satırlar buraya eklenebilir -->
                                        </tbody>
                                    </table>

                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <!-- Widgets End -->

                <!-- Footer Start -->
                <%- include('../admin-partials/footer.ejs') %>

                <!-- Footer End -->
            </div>
            <!-- Content End -->

            <!-- Back to Top -->

        </div>

        <!-- JavaScript Libraries -->
        <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
        <script
            src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
        <script
            src="/static/admin-panel-assets/lib/chart/chart.min.js"></script>
        <script
            src="/static/admin-panel-assets/lib/easing/easing.min.js"></script>
        <script
            src="/static/admin-panel-assets/lib/waypoints/waypoints.min.js"></script>
        <script
            src="/static/admin-panel-assets/lib/owlcarousel/owl.carousel.min.js"></script>
        <script
            src="/static/admin-panel-assets/lib/tempusdominus/js/moment.min.js"></script>
        <script
            src="/static/admin-panel-assets/lib/tempusdominus/js/moment-timezone.min.js"></script>
        <script
            src="/static/admin-panel-assets/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>

        <!-- Template Javascript -->
        <script src="/static/admin-panel-assets/js/main.js"></script>
    </body>

</html>