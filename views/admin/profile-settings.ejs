                                            <a href="#"
                                                class="card-link">Card
                                                link</a>
                                            <a href="#"
                                                class="card-link">Another
                                                link</a>
                                        </div> -->
                                        </div>
                                    </div>
                                    <div
                                        class="form-floating  text-center">
                                        <button type="submit"
                                            class="btn btn-primary w-50">Save</button>
                                    </div>
                                </form>

                            </div>

                        </div>

                    </div>

                </div>

                <div class="row">
                    <%- include('../admin-partials/footer.ejs') %>
                </div>
            </div>

